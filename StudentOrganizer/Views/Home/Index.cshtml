@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <div class="col-12">
        <div class="text-center">
            <h1 class="display-4">Welcome</h1>
        </div>
    </div>
</div>

<div class="row mt-3">
    <div class="col-8">
        <div class="custom-file">
            <label class="custom-file-label" for="fileLocation">Choose file</label>
            <input type="file" class="custom-file-input" id="fileLocation">
        </div>
    </div>
    <div class="col-4">
        <button type="button" class="btn btn-primary ml-3">Load Students</button>
    </div>
</div>

<div class="row mt-5">
    <div class="col-12">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">First Name</th>
                    <th scope="col">Last Name</th>
                    <th scope="col">Age</th>
                    <th scope="col">Status</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>
    

